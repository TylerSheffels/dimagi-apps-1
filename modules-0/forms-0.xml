<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Register New Patient</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/CD27A16C-D3FC-4DBA-A151-608D1D10A62B" uiVersion="1" version="206" name="Register New Patient">
					<patient_info_list>
						<name/>
						<mrd/>
						<bed/>
						<wife_of/>
						<age/>
						<admission_date/>
						<admission_time/>
						<parity/>
						<gravida/>
						<lmp/>
					</patient_info_list>
					<patient_info_cont>
						<edd/>
						<membrane_rupture/>
						<membrane_date/>
						<membrane_time/>
					</patient_info_cont>
					<t_f_labour>
						<true_false_checklist/>
						<false_checklist/>
						<true_labour/>
						<false_labour/>
					</t_f_labour>
					<high_risk_list>
						<age_risk_label/>
						<age_risk/>
						<age_no_risk/>
						<para_risk_label/>
						<para_risk/>
						<second_third_fourth_para_no_risk/>
						<height_risk/>
						<height_risk_label/>
						<weight_risk/>
						<weight_risk_label/>
						<prev_outcome/>
						<risk_history_label/>
						<risk_lbw/>
						<lbw_risk_label/>
						<prev_conditions/>
					</high_risk_list>
					<high_risk_page>
						<high_risk_score_label/>
						<high_risk_flag/>
					</high_risk_page>
					<grips>
						<fundal_grip/>
						<fundal_grip_other/>
						<fetal_lie/>
						<fetal_presentation/>
						<pelvic_grip_1/>
						<pelvic_grip_2/>
					</grips>
					<emergency_signs>
						<emergency_signs_list/>
					</emergency_signs>
					<es_alert>
						<alert/>
						<suggested_actions/>
						<choice/>
						<actions/>
					</es_alert>
					<false_labour_flag/>
					<age/>
					<parity/>
					<age_no_risk_0/>
					<age_risk_1/>
					<para_risk_1/>
					<para_no_risk_0/>
					<height_risk_1/>
					<weight_risk_3/>
					<risk_history_2/>
					<lbw_risk_1/>
					<risk_uterine_surgery_2/>
					<risk_aph_pph_2/>
					<risk_manual_placenta_2/>
					<risk_anaemia_3/>
					<risk_febrile_ailment_1/>
					<risk_hyptertension_2/>
					<risk_medical_condition_2/>
					<risk_bleeding_2/>
					<risk_presentation_2/>
					<risk_maturity_2/>
					<risk_prom_2/>
					<risk_fetal_distress_2/>
					<risk_prolonged_labour_2/>
					<risk_uterine_size_2/>
					<risk_dai_2/>
					<risk_pre_eclampsia_2/>
					<risk_eclampsia_2/>
					<high_risk_score/>
					<es_none/>
					<es_others/>
					<edd_calc/>
					<high_risk_flag/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>patient</case_type></create><update><admission_date/><admission_time/><age/><bed/><edd/><fetal_lie/><fetal_presentation/><fundal_grip/><gravida/><height_risk/><high_risk_flag/><lmp/><membrane_date/><membrane_time/><mrd/><parity/><pelvic_grip_1/><pelvic_grip_2/><prev_outcome/><risk_lbw/><weight_risk/><wife_of/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/patient_info_list"/>
			<bind nodeset="/data/patient_info_list/name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/patient_info_list/mrd" type="xsd:int" required="true()"/>
			<bind nodeset="/data/patient_info_list/bed" type="xsd:int" required="true()"/>
			<bind nodeset="/data/patient_info_list/wife_of" type="xsd:string" required="false()"/>
			<bind nodeset="/data/patient_info_list/age" type="xsd:int" required="true()"/>
			<bind nodeset="/data/patient_info_list/admission_date" type="xsd:date" required="true()"/>
			<bind nodeset="/data/patient_info_list/admission_time" type="xsd:time" required="true()"/>
			<bind nodeset="/data/patient_info_list/parity" required="true()"/>
			<bind nodeset="/data/patient_info_list/gravida" required="true()"/>
			<bind nodeset="/data/patient_info_list/lmp" type="xsd:date"/>
			<bind nodeset="/data/patient_info_cont"/>
			<bind nodeset="/data/patient_info_cont/edd" relevant="/data/patient_info_list/lmp != ''"/>
			<bind nodeset="/data/patient_info_cont/membrane_rupture"/>
			<bind nodeset="/data/patient_info_cont/membrane_date" type="xsd:date" relevant="/data/patient_info_cont/membrane_rupture = 'yes'"/>
			<bind nodeset="/data/patient_info_cont/membrane_time" type="xsd:time" relevant="/data/patient_info_cont/membrane_rupture = 'yes'"/>
			<bind nodeset="/data/t_f_labour"/>
			<bind nodeset="/data/t_f_labour/true_false_checklist"/>
			<bind nodeset="/data/t_f_labour/false_checklist"/>
			<bind nodeset="/data/t_f_labour/true_labour" relevant="count-selected(/data/t_f_labour/true_false_checklist) &gt; 0"/>
			<bind nodeset="/data/t_f_labour/false_labour" relevant="/data/false_labour_flag = 'yes'"/>
			<bind nodeset="/data/high_risk_list"/>
			<bind nodeset="/data/high_risk_list/age_risk_label" relevant="/data/age != &quot;&quot;"/>
			<bind nodeset="/data/high_risk_list/age_risk" relevant="/data/age_risk_1 = 1"/>
			<bind nodeset="/data/high_risk_list/age_no_risk" relevant="/data/age_no_risk_0 = 1"/>
			<bind nodeset="/data/high_risk_list/para_risk_label" relevant="/data/para_risk_1 = 1"/>
			<bind nodeset="/data/high_risk_list/para_risk" relevant="/data/para_risk_1 = 1"/>
			<bind nodeset="/data/high_risk_list/second_third_fourth_para_no_risk" relevant="/data/para_no_risk_0 = 1"/>
			<bind nodeset="/data/high_risk_list/height_risk" type="xsd:int"/>
			<bind nodeset="/data/high_risk_list/height_risk_label" relevant="/data/height_risk_1 = 1"/>
			<bind nodeset="/data/high_risk_list/weight_risk" type="xsd:int"/>
			<bind nodeset="/data/high_risk_list/weight_risk_label" relevant="/data/weight_risk_3 = 3"/>
			<bind nodeset="/data/high_risk_list/prev_outcome" relevant="/data/patient_info_list/gravida != 'primi'"/>
			<bind nodeset="/data/high_risk_list/risk_history_label" relevant="/data/risk_history_2 = 2"/>
			<bind nodeset="/data/high_risk_list/risk_lbw" relevant="/data/patient_info_list/parity != 'primi'"/>
			<bind nodeset="/data/high_risk_list/lbw_risk_label" relevant="/data/lbw_risk_1 = 1"/>
			<bind nodeset="/data/high_risk_list/prev_conditions" relevant="/data/patient_info_list/gravida != 'primi'"/>
			<bind nodeset="/data/high_risk_page"/>
			<bind nodeset="/data/high_risk_page/high_risk_score_label"/>
			<bind nodeset="/data/high_risk_page/high_risk_flag" relevant="/data/high_risk_score &gt; 3"/>
			<bind nodeset="/data/grips"/>
			<bind nodeset="/data/grips/fundal_grip"/>
			<bind nodeset="/data/grips/fundal_grip_other" type="xsd:string" relevant="/data/grips/fundal_grip = 'other'"/>
			<bind nodeset="/data/grips/fetal_lie"/>
			<bind nodeset="/data/grips/fetal_presentation"/>
			<bind nodeset="/data/grips/pelvic_grip_1"/>
			<bind nodeset="/data/grips/pelvic_grip_2"/>
			<bind nodeset="/data/emergency_signs"/>
			<bind nodeset="/data/emergency_signs/emergency_signs_list" constraint="(/data/es_none = 'yes' and /data/es_others = 'no') or (/data/es_none = 'no' and /data/es_others = 'yes') " jr:constraintMsg="jr:itext('emergency_signs/emergency_signs_list-constraintMsg')" required="false()"/>
			<bind nodeset="/data/es_alert" relevant="selected(/data/emergency_signs/emergency_signs_list, 'difficulty_breathing') or selected(/data/emergency_signs/emergency_signs_list, 'shock') or selected(/data/emergency_signs/emergency_signs_list, 'vaginal_bleeding') or selected(/data/emergency_signs/emergency_signs_list, 'convulsions_unconsciousness') or selected(/data/emergency_signs/emergency_signs_list, 'prolapsed_cord') or selected(/data/emergency_signs/emergency_signs_list, 'fetal_distress')"/>
			<bind nodeset="/data/es_alert/alert"/>
			<bind nodeset="/data/es_alert/suggested_actions"/>
			<bind nodeset="/data/es_alert/choice" required="true()"/>
			<bind nodeset="/data/es_alert/actions" type="xsd:string" relevant="/data/es_alert/choice = 'record_actions'"/>
			<bind nodeset="/data/false_labour_flag" calculate="if(count-selected(/data/t_f_labour/true_false_checklist) &lt; 1 and count-selected(/data/t_f_labour/false_checklist) &gt;= 1, 'yes', 'no')"/>
			<bind nodeset="/data/age" calculate="/data/patient_info_list/age"/>
			<bind nodeset="/data/parity" calculate="/data/patient_info_list/parity"/>
			<bind nodeset="/data/age_no_risk_0" calculate="if(/data/age &gt;= 18 or /data/age &lt;= 35, 1, 0)"/>
			<bind nodeset="/data/age_risk_1" calculate="if((/data/age &lt; 18) or (/data/age &gt; 35), 1, 0)"/>
			<bind nodeset="/data/para_risk_1" calculate="if(/data/patient_info_list/parity = &quot;primi&quot; or /data/patient_info_list/parity = &quot;multi&quot;, 1, 0)"/>
			<bind nodeset="/data/para_no_risk_0" calculate="if(/data/patient_info_list/parity = &quot;second&quot; or /data/patient_info_list/parity = &quot;third&quot; or /data/patient_info_list/parity = &quot;fourth&quot;,1,0)"/>
			<bind nodeset="/data/height_risk_1" calculate="if(/data/high_risk_list/height_risk &lt; 145, 1, 0)"/>
			<bind nodeset="/data/weight_risk_3" calculate="if((/data/high_risk_list/weight_risk &lt; 45) or (/data/high_risk_list/weight_risk &gt; 90), 3, 0)"/>
			<bind nodeset="/data/risk_history_2" calculate="if((/data/high_risk_list/prev_outcome = &quot;still_birth&quot;) or (/data/high_risk_list/prev_outcome = &quot;miscarriage&quot;) or (/data/high_risk_list/prev_outcome = &quot;spont_abortion&quot;), 2, 0)"/>
			<bind nodeset="/data/lbw_risk_1" calculate="if(/data/high_risk_list/risk_lbw = &quot;yes&quot;, 1, 0)"/>
			<bind nodeset="/data/risk_uterine_surgery_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;uterine_surgery&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_aph_pph_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;aph_pph&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_manual_placenta_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;manual_placenta&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_anaemia_3" calculate="if(/data/high_risk_list/prev_conditions = &quot;anaemia&quot;, 3, 0)"/>
			<bind nodeset="/data/risk_febrile_ailment_1" calculate="if(/data/high_risk_list/prev_conditions = &quot;febrile_ailment&quot;, 1, 0)"/>
			<bind nodeset="/data/risk_hyptertension_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;hypertension&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_medical_condition_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;medical_condition&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_bleeding_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;bleeding&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_presentation_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;presentation&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_maturity_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;maturity&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_prom_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;prom&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_fetal_distress_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;fetal_distress&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_prolonged_labour_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;prolonged_labour&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_uterine_size_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;uterine_size&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_dai_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;dai&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_pre_eclampsia_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;pre_eclampsia&quot;, 2, 0)"/>
			<bind nodeset="/data/risk_eclampsia_2" calculate="if(/data/high_risk_list/prev_conditions = &quot;eclampsia&quot;, 2, 0)"/>
			<bind nodeset="/data/high_risk_score" calculate="/data/age_risk_1+/data/para_risk_1+/data/height_risk_1+/data/weight_risk_3+/data/risk_history_2+/data/lbw_risk_1+/data/risk_uterine_surgery_2+/data/risk_aph_pph_2+/data/risk_manual_placenta_2+/data/risk_anaemia_3+/data/risk_febrile_ailment_1+/data/risk_hyptertension_2+/data/risk_medical_condition_2+/data/risk_bleeding_2+/data/risk_presentation_2+/data/risk_maturity_2+/data/risk_prom_2+/data/risk_fetal_distress_2+/data/risk_prolonged_labour_2+/data/risk_uterine_size_2+/data/risk_dai_2+/data/risk_pre_eclampsia_2+/data/risk_eclampsia_2"/>
			<bind nodeset="/data/es_none" calculate="if(/data/emergency_signs/emergency_signs_list = &quot;none&quot;, 'yes', 'no')"/>
			<bind nodeset="/data/es_others" calculate="if(/data/emergency_signs/emergency_signs_list = &quot;difficulty_breathing&quot; or /data/emergency_signs/emergency_signs_list = &quot;shock&quot; or /data/emergency_signs/emergency_signs_list = &quot;vaginal_bleeding&quot; or /data/emergency_signs/emergency_signs_list = &quot;convulsions_unconsciousness&quot; or /data/emergency_signs/emergency_signs_list = &quot;prolapsed_cord&quot; or /data/emergency_signs/emergency_signs_list = &quot;fetal_distress&quot;, 'yes', 'no')"/>
			<bind nodeset="/data/edd_calc" calculate="date(/data/patient_info_list/lmp + 274)"/>
			<bind nodeset="/data/high_risk_flag" calculate="if(/data/high_risk_score &gt; 4, 'yes', 'no')"/>
