<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Update Patient</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/229F482D-25ED-4462-B6CA-2AEA28F34296" uiVersion="1" version="83" name="Update Patient">
					<name/>
					<gender/>
					<dob/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><case_name/><dob/><gender/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/name" type="xsd:string"/>
			<bind nodeset="/data/gender"/>
			<bind nodeset="/data/dob" type="xsd:date"/>
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>What is patient's name?</value>
					</text>
					<text id="gender-label">
						<value>What is patient's gender?</value>
					</text>
					<text id="gender-female-label">
						<value>Female</value>
					</text>
					<text id="gender-male-label">
						<value>Male</value>
					</text>
					<text id="dob-label">
						<value>When was patient born?</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/name" nodeset="/data/case/update/case_name" relevant="count(/data/name) &gt; 0"/><bind calculate="/data/dob" nodeset="/data/case/update/dob" relevant="count(/data/dob) &gt; 0"/><bind calculate="/data/gender" nodeset="/data/case/update/gender" relevant="count(/data/gender) &gt; 0"/><setvalue event="xforms-ready" ref="/data/dob" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/dob"/><setvalue event="xforms-ready" ref="/data/gender" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/gender"/><setvalue event="xforms-ready" ref="/data/name" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/case_name"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<input ref="/data/name">
			<label ref="jr:itext('name-label')"/>
		</input>
		<select1 ref="/data/gender">
			<label ref="jr:itext('gender-label')"/>
			<item>
				<label ref="jr:itext('gender-female-label')"/>
				<value>female</value>
			</item>
			<item>
				<label ref="jr:itext('gender-male-label')"/>
				<value>male</value>
			</item>
		</select1>
		<input ref="/data/dob">
			<label ref="jr:itext('dob-label')"/>
		</input>
	</h:body>
</h:html>